{
    "name": "wp-performance/wp-performance",
    "description": "WP Performance is a cache & performance plugin",
    "type": "wordpress-plugin",
    "keywords": [
		"wordpress",
		"cache",
		"performance",
		"speed optimization"
    ],
    "homepage": "https://www.wp-performance.com",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Ante Laca",
            "email": "ante.laca@gmail.com",
            "homepage": "https://antelaca.xyz"
        }
    ],
    "require": {
        "php": ">=5.6",
        "matthiasmullie/minify": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "WPP\\": "includes/classes",
            "WPP\\Addons\\": "includes/addons"
        },
        "files": ["includes/loader.php"],
        "classmap": [
			"includes/classes",
			"includes/addons"
        ]
    }
}